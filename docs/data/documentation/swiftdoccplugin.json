      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The Swift-DocC plugin is a Swift Package Manager command plugin that supports building",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "documentation for SwiftPM libraries and executables.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "To use the Swift-DocC plugin with your package, first add it as a dependency:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let package = Package(",
            "    \/\/ name, platforms, products, etc.",
            "    dependencies: [",
            "        \/\/ other dependencies",
            "        .package(url: \"https:\/\/github.com\/apple\/swift-docc-plugin\", from: \"1.0.0\"),",
            "    ],",
            "    targets: [",
            "        \/\/ targets",
            "    ]",
            ")"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Swift 5.6 or higher is required in order to run the plugin.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "Then, build documentation for the libraries and executables in that package and its dependencies by running the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "following from the command-line:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "$ swift package generate-documentation"
          ],
          "syntax" : "shell",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The documentation on this site is focused on the Swift-DocC plugin specifically. For more",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "general documentation on how to use Swift-DocC, see the documentation",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/www.swift.org\/documentation\/docc\/",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
